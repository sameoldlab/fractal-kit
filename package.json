{
	"name": "@fractl-ui/evm",
	"version": "0.0.1",
	"description": "",
	"keywords": [],
	"author": "sameoldlab",
	"license": "MIT",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "mitosis build -c mitosis.config.cjs",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"dependencies": {
		"@builder.io/mitosis": "^0.0.137",
		"@sveltejs/adapter-auto": "^3.1.0",
		"@wagmi/core": "2.0.0-rc.1",
		"publint": "^0.2.7",
		"viem": "2.0.0-rc.2"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@builder.io/eslint-plugin-mitosis": "^0.0.15",
		"@builder.io/mitosis-cli": "^0.0.96",
		"@sveltejs/adapter-vercel": "^4.0.3",
		"@sveltejs/enhanced-img": "^0.1.7",
		"@sveltejs/kit": "^2.0.4",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tsconfig/svelte": "^5.0.2",
		"eslint": "^8.56.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
